{
	  "name"            : "node-cloudant"
	, "description"     : "A node.js client library for Cloudant"
	, "keywords"        : ["cloudant", "couchdb", "bigcouch", "db"]
	, "version"         : "0.1.1"
	, "author"          : "Ronny Haase <ronhaase@gmail.com>"
	, "homepage"        : "http://ronnyhaase.github.com/node-cloudant/"
	, "contributors"    : [
	]
	, "licenses"        : [
		{ 
			  "type" : "Mozilla Public License 2.0 (MPL-2.0)"
			, "url"  : "http://opensource.org/licenses/MPL-2.0"
		}
	]
	, "repository"      : {
		  "type" : "git"
		, "url"  : "https://github.com/ronnyhaase/node-cloudant"
	}
	, "bugs"            : {
		"url": "https://github.com/ronnyhaase/node-cloudant/issues"
	}

	, "dependencies"    : {
		  "lodash"  : "latest"
		, "q"       : "latest"
		, "request" : "latest"
	}
	, "devDependencies" : {
		  "mocha"  : "latest"
		, "jshint" : "latest"
		, "jslint" : "latest"
	}
	, "config"          : {
		"native" : false
	}
	, "main"            : "./lib/cloudant"
	, "directories"     : {
		  "lib"  : "./lib/cloudant"
		, "test" : "test"
	}
	, "engines"         : {
		"node" : ">=0.8.11"
	}
	, "scripts": {
		"test": "./node_modules/jshint/bin/hint ./lib/*.js ./lib/cloudant/*.js ./test/*.js"
	}
}
